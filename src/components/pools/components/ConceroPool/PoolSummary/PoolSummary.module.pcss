.summaryContainer {
	margin-top: 48px;
	display: flex;
	flex-direction: column;
	gap: var(--sp-sm);

	h5 {
		color: var(--color-grey-600);
	}
}

.summaryContent {
	height: 100%;
	max-height: 308px;
	flex-direction: row;
	gap: var(--sp-lg);

	> div {
		width: 49%;
	}
}

.sectionTitle {
	color: var(--color-grey-600);
	font-weight: 500;
	font-size: 0.875rem;
}
