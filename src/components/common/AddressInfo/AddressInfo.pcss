.asset_info_container {
	display: flex;
	flex-direction: row;
	justify-content: center;
	gap: var(--sp-xs);
}

.asset_info_title {
	color: var(--color-grey-600);
	line-height: 18px;
	font-weight: 400;
	font-size: 0.875rem;
}

.asset_info_error {
	font-size: 0.875rem;
	color: var(--color-danger-600);
}
