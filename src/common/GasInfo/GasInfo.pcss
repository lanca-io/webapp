.gas_info {
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	align-items: center;
	align-self: stretch;
}

.gas_info_description {
	display: flex;
	flex-direction: row;
	align-items: center;
	gap: var(--sp-sm);
}

.gas_info_text {
	font-size: 1rem;
	font-weight: 400;
	line-height: 20px;
	font-style: normal;
	color: var(--color-grey-600);
}

.gas_info_value {
	font-size: 1rem;
	font-weight: 500;
	line-height: 20px;
	font-style: normal;
	color: var(--color-grey-700);
}
